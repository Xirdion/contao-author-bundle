{
    "name": "dreibein/contao-author-bundle",
    "description": "Show additional author information on a reader page",
    "type": "contao-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Agentur Dreibein",
            "homepage": "https://www.agentur-dreibein.de",
            "email": "info@agentur-dreibein.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dreibein/contao-author-bundle/issues",
        "source": "https://github.com/dreibein/contao-author-bundle"
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "contao/calendar-bundle": "^4.9",
        "contao/core-bundle": "^4.9",
        "contao/faq-bundle": "^4.9",
        "contao/news-bundle": "^4.9"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "contao/manager-plugin": "^2.0",
        "contao/test-case": "^4.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.5"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "extra": {
        "contao-manager-plugin": "Dreibein\\ContaoAuthorBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Dreibein\\ContaoAuthorBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dreibein\\ContaoAuthorBundle\\Tests\\": "tests"
        }
    }
}
